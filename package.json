{
  "name": "chimp-gpt",
  "version": "1.0.2",
  "description": "A Discord chatbot leveraging the OpenAI GPT-3.5 API for dynamic and intelligent responses.",
  "main": "chimpGPT.js",
  "private": true,
  "scripts": {
    "test": "node tests/testRunner.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prestart": "echo 'Starting the app...'",
    "start": "nodemon chimpGPT.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/f0rky/Chimp-GPT.git"
  },
  "keywords": [
    "discord",
    "chatbot",
    "openai",
    "gpt-3.5",
    "ai",
    "machine-learning",
    "bot",
    "nodejs",
    "natural-language-processing",
    "nlp"
  ],
  "author": {
    "name": "Brett Healy",
    "email": "brett.healy@data.net.nz"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.10.1",
    "axios": "^1.8.4",
    "completions": "^2.7.0",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "moment-timezone": "^0.5.43",
    "openai": "^4.77.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "rate-limiter-flexible": "^6.2.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^8.0.0",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.3"
  }
}
