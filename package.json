{
  "name": "chimp-gpt",
  "version": "1.5.0",
  "description": "A Discord chatbot leveraging the OpenAI GPT-3.5 API for dynamic and intelligent responses.",
  "main": "chimpGPT.js",
  "private": true,
  "scripts": {
    "test": "node tests/testRunner.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prestart": "echo 'Starting the app...'",
    "start": "nodemon --ignore data/ --ignore logs/ chimpGPT.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/f0rky/Chimp-GPT.git"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "jsxBracketSameLine": false,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "embeddedLanguageFormatting": "auto"
  },
  "keywords": [
    "discord",
    "chatbot",
    "openai",
    "gpt-3.5",
    "ai",
    "machine-learning",
    "bot",
    "nodejs",
    "natural-language-processing",
    "nlp"
  ],
  "author": {
    "name": "Brett Healy",
    "email": "brett.healy@data.net.nz"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.11.2",
    "axios": "^1.9.0",
    "completions": "^2.7.0",
    "cors": "^2.8.5",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "moment-timezone": "^0.5.48",
    "openai": "^4.100.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "rate-limiter-flexible": "^7.1.0",
    "undici": "^6.21.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "canvas": "^3.1.0",
    "chai": "^5.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.0.0",
    "mocha": "^11.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3"
  },
  "overrides": {
    "undici": "^6.21.2"
  }
}
